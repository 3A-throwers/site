  },
  "homepage": "https://github.com/3A-Throwers/site#readme",
  "dependencies": {
    "@vscode/markdown-it-katex": "^1.0.0",
    "markdown-it-footnote": "^3.0.3"
  },
  "devDependencies": {
    "@types/markdown-it-footnote": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "vitepress": "1.0.0-rc.14",
    "vue": "^3.3.4"
  },
  "pnpm": {
